{
  "name": "ScriptBag",
  "version": "1.0",
  "description": "Temporally description",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "./public/popup.html",
    "default_title": "ScriptBag"
  }
}
